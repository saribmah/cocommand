      "delete": {
        "tags": [
          "oauth"
        ],
        "operationId": "delete_tokens",
        "parameters": [
          {
            "name": "ext",
            "in": "path",
            "description": "Extension identifier",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "provider",
            "in": "path",
            "description": "OAuth provider name",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Tokens deleted successfully"
