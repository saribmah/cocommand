    "/extension/{extension_id}/invoke/{tool_id}": {
      "post": {
        "tags": [
          "extensions"
        ],
        "summary": "POST /extension/{extension_id}/invoke/{tool_id}",
        "description": "Looks up the extension and tool by ID, executes the tool with the provided\nJSON input, and returns a standardised `{ ok, data }` / `{ ok, error }` envelope.",
        "operationId": "invoke_tool",
        "parameters": [
          {
            "name": "extension_id",
            "in": "path",
            "description": "Extension identifier",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "tool_id",
            "in": "path",
            "description": "Tool identifier",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {}
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Tool execution result envelope with { ok, data }"
          },
          "400": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiErrorResponse"
                }
              }
            }
          },
          "404": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiErrorResponse"
                }
              }
            }
          }
        }
      }
    },
