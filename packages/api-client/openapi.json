  "components": {
    "schemas": {
      "ApiErrorBody": {
        "type": "object",
        "required": [
          "code",
          "message"
        ],
        "properties": {
          "code": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
      },
      "ApiErrorResponse": {
        "type": "object",
        "required": [
          "ok",
          "error"
        ],
        "properties": {
          "error": {
            "$ref": "#/components/schemas/ApiErrorBody"
          },
          "ok": {
            "type": "boolean"
          }
        }
      },
      "ApiSessionContext": {
        "type": "object",
        "required": [
          "workspace_id",
          "session_id",
          "started_at"
        ],
        "properties": {
          "ended_at": {
            "type": [
              "integer",
              "null"
            ],
            "format": "int64",
            "minimum": 0
          },
          "session_id": {
            "type": "string"
          },
          "started_at": {
            "type": "integer",
            "format": "int64",
            "minimum": 0
          },
          "workspace_id": {
            "type": "string"
          }
        }
      },
      "ApplicationInfo": {
        "type": "object",
        "required": [
          "id",
          "name",
          "path"
        ],
        "properties": {
          "bundleId": {
            "type": [
              "string",
              "null"
            ]
          },
          "icon": {
            "type": [
              "string",
              "null"
            ]
          },
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "path": {
            "type": "string"
          }
        }
      },
      "ApplicationsResponse": {
        "type": "object",
        "required": [
          "applications",
          "count"
        ],
        "properties": {
          "applications": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ApplicationInfo"
            }
          },
          "count": {
            "type": "integer",
            "minimum": 0
          }
        }
      },
      "BrowserStatus": {
        "type": "object",
        "required": [
          "connected"
        ],
        "properties": {
          "connected": {
            "type": "boolean"
          }
        }
      },
      "CoreEvent": {
        "oneOf": [
          {
            "type": "object",
            "required": [
              "payload",
              "type"
            ],
            "properties": {
              "payload": {
                "$ref": "#/components/schemas/SessionPartUpdatedPayload"
              },
              "type": {
                "type": "string",
                "enum": [
                  "SessionPartUpdated"
                ]
              }
            }
          },
          {
            "type": "object",
            "required": [
              "payload",
              "type"
            ],
            "properties": {
              "payload": {
                "$ref": "#/components/schemas/SessionContextPayload"
              },
              "type": {
                "type": "string",
                "enum": [
                  "SessionContextUpdated"
                ]
              }
            }
          }
        ]
      },
      "ExtensionInfo": {
        "type": "object",
        "required": [
          "id",
          "name",
          "kind",
          "status",
          "tags",
          "tools"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "kind": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "tools": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ExtensionToolInfo"
            }
          },
          "view": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/ExtensionViewInfo"
              }
            ]
          }
        }
      },
      "ExtensionPart": {
        "allOf": [
          {
            "$ref": "#/components/schemas/PartBase"
          },
          {
            "type": "object",
            "required": [
              "extensionId",
              "name"
            ],
            "properties": {
              "extensionId": {
                "type": "string"
              },
              "kind": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "name": {
                "type": "string"
              },
              "source": {
                "oneOf": [
                  {
                    "type": "null"
                  },
                  {
                    "$ref": "#/components/schemas/FilePartSourceText"
                  }
                ]
              }
            }
          }
        ]
      },
      "ExtensionToolInfo": {
        "type": "object",
        "required": [
          "id",
          "name",
          "input_schema"
        ],
        "properties": {
          "description": {
            "type": [
              "string",
              "null"
            ]
          },
          "id": {
            "type": "string"
          },
          "input_schema": {},
          "name": {
            "type": "string"
          }
        }
      },
      "ExtensionViewInfo": {
        "type": "object",
        "required": [
          "entry",
          "label"
        ],
        "properties": {
          "entry": {
            "type": "string"
          },
          "label": {
            "type": "string"
          },
          "popout": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/ExtensionViewPopoutInfo"
              }
            ]
          }
        }
      },
      "ExtensionViewPopoutInfo": {
        "type": "object",
        "required": [
          "width",
          "height",
          "title"
        ],
        "properties": {
          "height": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "title": {
            "type": "string"
          },
          "width": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          }
        }
      },
      "FilePart": {
        "allOf": [
          {
            "$ref": "#/components/schemas/PartBase"
          },
          {
            "type": "object",
            "required": [
              "base64",
              "mediaType"
            ],
            "properties": {
              "base64": {
                "type": "string"
              },
              "mediaType": {
                "type": "string"
              },
              "name": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "source": {
                "oneOf": [
                  {
                    "type": "null"
                  },
                  {
                    "$ref": "#/components/schemas/FilePartSource"
                  }
                ]
              }
            }
          }
        ]
      },
      "FilePartFileSource": {
        "type": "object",
        "required": [
          "text",
          "path"
        ],
        "properties": {
          "path": {
            "type": "string"
          },
          "text": {
            "$ref": "#/components/schemas/FilePartSourceText"
          }
        }
      },
      "FilePartSource": {
        "oneOf": [
          {
            "allOf": [
              {
                "$ref": "#/components/schemas/FilePartFileSource"
              },
              {
                "type": "object",
                "required": [
                  "type"
                ],
                "properties": {
                  "type": {
                    "type": "string",
                    "enum": [
                      "file"
                    ]
                  }
                }
              }
            ]
          },
          {
            "allOf": [
              {
                "$ref": "#/components/schemas/FilePartSymbolSource"
              },
              {
                "type": "object",
                "required": [
                  "type"
                ],
                "properties": {
                  "type": {
                    "type": "string",
                    "enum": [
                      "symbol"
                    ]
                  }
                }
              }
            ]
          }
        ]
      },
      "FilePartSourceText": {
        "type": "object",
        "required": [
          "value",
          "start",
          "end"
        ],
        "properties": {
          "end": {
            "type": "integer",
            "format": "int64"
          },
          "start": {
            "type": "integer",
            "format": "int64"
          },
          "value": {
            "type": "string"
          }
        }
      },
      "FilePartSymbolSource": {
        "type": "object",
        "required": [
          "text",
          "path",
          "name",
          "kind"
        ],
        "properties": {
          "kind": {
            "type": "integer",
            "format": "int64"
          },
          "name": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "text": {
            "$ref": "#/components/schemas/FilePartSourceText"
          }
        }
      },
      "MessagePart": {
        "oneOf": [
          {
            "allOf": [
              {
                "$ref": "#/components/schemas/TextPart"
              },
              {
                "type": "object",
                "required": [
                  "type"
                ],
                "properties": {
                  "type": {
                    "type": "string",
                    "enum": [
                      "text"
                    ]
                  }
                }
              }
            ]
          },
          {
            "allOf": [
              {
                "$ref": "#/components/schemas/ReasoningPart"
