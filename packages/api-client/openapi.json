                }
              }
            ]
          },
          {
            "allOf": [
              {
                "$ref": "#/components/schemas/ExtensionPart"
              },
              {
                "type": "object",
                "required": [
                  "type"
                ],
                "properties": {
                  "type": {
                    "type": "string",
                    "enum": [
                      "extension"
                    ]
