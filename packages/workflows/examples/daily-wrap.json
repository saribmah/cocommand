{
  "id": "daily-wrap",
  "name": "Daily Wrap",
  "description": "Summarize today's schedule and create a quick note.",
  "version": "1.0.0",
  "steps": [
    {
      "id": "calendar-summary",
      "commandId": "calendar-today",
      "inputs": {}
    },
    {
      "id": "quick-note",
      "commandId": "quick-note",
      "inputs": {
        "text": "Summary of today: {{steps.calendar-summary.outputs.summary}}"
      }
    }
  ],
  "permissions": {
    "calendar-today": "read",
    "quick-note": "write"
  }
}
